{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "arrow-body-style": 0,
    "max-len": [1, 120],
    "no-use-before-define": 0,
    "no-plusplus": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "import/first": 0,
    "comma-dangle": 0,
    "jsx-a11y/label-has-for": 0
  }
}
